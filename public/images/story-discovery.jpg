<svg width="1600" height="1000" viewBox="0 0 1600 1000" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Tech background -->
  <rect width="1600" height="1000" fill="url(#discovery-bg)"/>
  
  <!-- Circuit board pattern -->
  <g opacity="0.15">
    <line x1="200" y1="300" x2="400" y2="300" stroke="#06b6d4" stroke-width="2"/>
    <line x1="400" y1="300" x2="400" y2="500" stroke="#06b6d4" stroke-width="2"/>
    <line x1="400" y1="500" x2="600" y2="500" stroke="#8b5cf6" stroke-width="2"/>
    <circle cx="200" cy="300" r="8" fill="#06b6d4"/>
    <circle cx="400" cy="300" r="8" fill="#06b6d4"/>
    <circle cx="400" cy="500" r="8" fill="#8b5cf6"/>
    <circle cx="600" cy="500" r="8" fill="#8b5cf6"/>
  </g>
  
  <!-- Central AI brain/network -->
  <circle cx="800" cy="500" r="150" fill="url(#ai-glow)" opacity="0.3"/>
  <circle cx="800" cy="500" r="120" fill="none" stroke="url(#gradient-stroke)" stroke-width="3" opacity="0.6"/>
  <circle cx="800" cy="500" r="80" fill="none" stroke="url(#gradient-stroke)" stroke-width="2" opacity="0.4"/>
  
  <!-- Neural network nodes -->
  <g opacity="0.6">
    <circle cx="700" cy="400" r="12" fill="#06b6d4"/>
    <circle cx="900" cy="400" r="12" fill="#8b5cf6"/>
    <circle cx="700" cy="600" r="12" fill="#8b5cf6"/>
    <circle cx="900" cy="600" r="12" fill="#06b6d4"/>
    <circle cx="650" cy="500" r="10" fill="#a855f7"/>
    <circle cx="950" cy="500" r="10" fill="#0ea5e9"/>
    
    <!-- Connection lines -->
    <line x1="700" y1="400" x2="800" y2="500" stroke="#06b6d4" stroke-width="2" opacity="0.4"/>
    <line x1="900" y1="400" x2="800" y2="500" stroke="#8b5cf6" stroke-width="2" opacity="0.4"/>
    <line x1="700" y1="600" x2="800" y2="500" stroke="#8b5cf6" stroke-width="2" opacity="0.4"/>
    <line x1="900" y1="600" x2="800" y2="500" stroke="#06b6d4" stroke-width="2" opacity="0.4"/>
  </g>
  
  <!-- Data visualization bars -->
  <g transform="translate(1100, 300)" opacity="0.5">
    <rect x="0" y="250" width="60" height="150" rx="8" fill="#06b6d4"/>
    <rect x="80" y="180" width="60" height="220" rx="8" fill="#8b5cf6"/>
    <rect x="160" y="200" width="60" height="200" rx="8" fill="#0ea5e9"/>
    <rect x="240" y="150" width="60" height="250" rx="8" fill="#a855f7"/>
  </g>
  
  <!-- Light bulb moment -->
  <g transform="translate(300, 150)" opacity="0.7">
    <circle cx="0" cy="0" r="50" fill="url(#bulb-glow)"/>
    <path d="M -20 -10 L -20 20 L 20 20 L 20 -10" fill="none" stroke="#fbbf24" stroke-width="3"/>
    <line x1="-30" y1="-30" x2="-45" y2="-45" stroke="#fbbf24" stroke-width="3" stroke-linecap="round"/>
    <line x1="30" y1="-30" x2="45" y2="-45" stroke="#fbbf24" stroke-width="3" stroke-linecap="round"/>
    <line x1="0" y1="-40" x2="0" y2="-60" stroke="#fbbf24" stroke-width="3" stroke-linecap="round"/>
  </g>
  
  <!-- Floating particles -->
  <circle cx="500" cy="200" r="4" fill="#06b6d4" opacity="0.6"/>
  <circle cx="1200" cy="700" r="4" fill="#8b5cf6" opacity="0.6"/>
  <circle cx="400" cy="800" r="3" fill="#a855f7" opacity="0.5"/>
  <circle cx="1300" cy="250" r="3" fill="#0ea5e9" opacity="0.5"/>
  
  <defs>
    <linearGradient id="discovery-bg" x1="0" y1="0" x2="1600" y2="1000">
      <stop offset="0%" stop-color="#0f172a"/>
      <stop offset="50%" stop-color="#1e293b"/>
      <stop offset="100%" stop-color="#0f172a"/>
    </linearGradient>
    <radialGradient id="ai-glow">
      <stop offset="0%" stop-color="#06b6d4"/>
      <stop offset="50%" stop-color="#8b5cf6"/>
      <stop offset="100%" stop-color="#06b6d4" stop-opacity="0"/>
    </radialGradient>
    <linearGradient id="gradient-stroke" x1="0" y1="0" x2="1" y2="1">
      <stop offset="0%" stop-color="#06b6d4"/>
      <stop offset="100%" stop-color="#8b5cf6"/>
    </linearGradient>
    <radialGradient id="bulb-glow">
      <stop offset="0%" stop-color="#fbbf24" stop-opacity="0.8"/>
      <stop offset="100%" stop-color="#fbbf24" stop-opacity="0"/>
    </radialGradient>
  </defs>
</svg>
